@model ProjetoAulaBackEnd.Models.Imovel

@{
    ViewData["Title"] = "Delete";
}


<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined" rel="stylesheet">
    <link rel="stylesheet" href="https://demo.haeckdesign.com/get-social-icon-set/style.css">
    
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Quicksand:wght@400;700&display=swap" rel="stylesheet">

    <link rel="icon" type="icon/ico" href="~/img/favicon-32x32.png">
    <link rel="stylesheet" href="~/components/css/button.css">    
    <link rel="stylesheet" href="~/css/stylepage.css">        

    <title>Deletar Imóvel | CarpeDiem</title>
</head>

<body>
    <h1 class="flex flex-wrap j-start ai-center gap-3 mt-1 bold-7 t-black font-size-7">Deletar Imóvel</h1>

    <div>
        <p class="py-2 flex flex-wrap j-start ai-center gap-3 mt-1 bold-7 t-black font-size-6">Tem certeza que deseja excluir esse imóvel?</p>
    </div>

    <p class="bold-7 t-black font-size-5">Imovel</p>
    <hr />

    <div>    
        <dl class="row">
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Hospede)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Hospede.CPF)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Endereco)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Endereco)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Bairro)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Bairro)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Cidade)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Cidade)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.TipoEsp)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.TipoEsp)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.TipoAc)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.TipoAc)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.QtdeHospedes)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.QtdeHospedes)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.QtdeCamas)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.QtdeCamas)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.QtdeQuartos)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.QtdeQuartos)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.QtdeBanheiros)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.QtdeBanheiros)
            </dd>        
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.CheckIn)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.CheckIn)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.CheckOut)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.CheckOut)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.DistanciaPraia)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.DistanciaPraia)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.ValorDiaria)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.ValorDiaria)
            </dd>        
        </dl>
    
        <form asp-action="Delete">
            <input type="hidden" asp-for="IdImovel" />
            <input type="submit" value="Deletar" class="btn btn-danger" /> |
            <a asp-action="Index" class="btn btn-blue btn-effect">Voltar para Lista</a>
        </form>
    </div>

    <div class="copyright p-2 t-center"></div>  
        
    <script src="~/js/copyright.js"></script>

</body>
</html>






